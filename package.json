{
  "name": "trouve.fastify",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "start:dev": "nodemon",
    "start:production": "NODE_ENV=production node dist/server.js",
    "start:staging": "NODE_ENV=staging node -r tsconfig-paths/register dist/server.js",
    "build": "tsc -p tsconfig.json",
    "knex": "./node_modules/.bin/knex",
    "knex:migrate:make": "knex --knexfile src/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "knex:migrate:down": "knex --knexfile src/knexfile.ts migrate:down",
    "lint": "eslint . --ext .ts --ignore-pattern .gitignore",
    "lint:fix": "eslint . --ext .ts --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.2",
    "fastify-mongodb": "^4.1.1",
    "ioredis": "^4.28.5",
    "joi": "^17.6.0",
    "joi-phone-number": "^5.1.0",
    "joi-typescript-validator": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "sequelize-fastify": "^1.0.9",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "fastify-tsconfig": "^1.0.1",
    "ts-node": "^10.6.0"
  }
}
